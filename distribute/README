========================================================
        Quick Start
========================================================
-- from terminal -- 
 sudo ./install.sh

-- from Comsol -- 
 start Comsol 
 open examples/mincde/coli.mph
 open MATLAB from comsol
      File > Client/Server/MATLAB > Connect to MATLAB
 export mesh to MATALB
      File > Export > Export FEM Structure as 'fem'

-- from Matlab --
 cd examples/mincde
 fem=urdme(fem,'fange');
 postplot(fem,'Tetdata','MinD_m')
========================================================
